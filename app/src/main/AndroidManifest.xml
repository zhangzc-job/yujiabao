<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qdjiaotong.yujiabao">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".YuJiaBaoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.YuJiaBao">
        <activity
            android:name=".activity.login.ui.LoginActivity"
            android:label="@string/title_activity_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name="com.qdjiaotong.yujiabao.activity.web_activity.webview.WebViewActivity"
            android:label="@string/title_activity_web_view" />

        <activity android:name="com.qdjiaotong.yujiabao.activity.mytangkou.MyTangKouActivity" />
        <activity android:name=".MainActivity">

        </activity>
    </application>

</manifest>